-- üòà AVENL | TROLLS PLAYER
-- ===============================

local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = Players.LocalPlayer

local TrollTab = Window:MakeTab({ "Trolls Player", "skull" })
TrollTab:AddSection({ "Trolls Diretos üòà" })

local TargetName = ""

TrollTab:AddTextBox({
	Name = "Nome do Player",
	PlaceholderText = "Ex: Player123",
	Callback = function(v)
		TargetName = v
	end
})

local function getTarget()
	for _, plr in pairs(Players:GetPlayers()) do
		if plr.Name:lower():find(TargetName:lower()) then
			return plr
		end
	end
end

local Remote = ReplicatedStorage:FindFirstChildWhichIsA("RemoteEvent")

-- ‚ò†Ô∏è KILL (REMOTE)
TrollTab:AddButton({
	Name = "Kill (Remote)",
	Callback = function()
		if Remote then
			Remote:FireServer("Kill", TargetName)
		end
	end
})

-- ‚ò†Ô∏è KILL (TOOL)
TrollTab:AddButton({
	Name = "Kill (Tool)",
	Callback = function()
		local tool = player.Backpack:FindFirstChild("Ball") or player.Backpack:FindFirstChild("Kill")
		if tool then
			tool.Parent = player.Character
			tool:Activate()
		end
	end
})

-- üõãÔ∏è BRING (REMOTE)
TrollTab:AddButton({
	Name = "Bring (Remote)",
	Callback = function()
		if Remote then
			Remote:FireServer("Bring", TargetName)
		end
	end
})

-- üõãÔ∏è BRING (COUCH)
TrollTab:AddButton({
	Name = "Bring (Couch)",
	Callback = function()
		local tool = player.Backpack:FindFirstChild("Couch")
		if tool then
			tool.Parent = player.Character
			tool:Activate()
		end
	end
})-- ===============================
-- üõ°Ô∏è AVENL | ADMIN
-- ===============================

local AdminTab = Window:MakeTab({ "Admin", "shield" })
AdminTab:AddSection({ "Admin por Chat üõ°Ô∏è" })

local AdminON = false

AdminTab:AddToggle({
	Name = "Ativar Admin Chat",
	Default = false,
	Callback = function(v)
		AdminON = v
	end
})

player.Chatted:Connect(function(msg)
	if not AdminON then return end

	local args = msg:split(" ")
	local cmd = args[1]
	local name = args[2]

	for _, plr in pairs(Players:GetPlayers()) do
		if name and plr.Name:lower():find(name:lower()) then

			if cmd == "!bring" then
				local couch = player.Backpack:FindFirstChild("Couch")
				if couch then
					couch.Parent = player.Character
					couch:Activate()
				end
			end

			if cmd == "!kill" then
				local ball = player.Backpack:FindFirstChild("Ball")
				if ball then
					ball.Parent = player.Character
					ball:Activate()
				end
			end

			if cmd == "!view" and plr.Character then
				workspace.CurrentCamera.CameraSubject = plr.Character:FindFirstChild("Humanoid")
			end
		end
	end
end)-- ===============================
