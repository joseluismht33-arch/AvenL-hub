local RunService = game:GetService("RunService")
RunService.RenderStepped:Connect(function()
	if getgenv().Fly and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
		player.Character.HumanoidRootPart.Velocity = Vector3.new(0, 50, 0)
	end
end)local MainTab = Window:MakeTab({ "Main", "home" }) 
MainTab:AddSection({ "Main Functions" }) 

MainTab:AddButton({ 
	Name = "Infinite Jump", 
	Callback = function() 
		getgenv().InfiniteJump = true 
	end 
}) 

UserInputService.JumpRequest:Connect(function() 
	if getgenv().InfiniteJump then 
		player.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping") 
	end 
end)local VisualTab = Window:MakeTab({ "Visual", "eye" })
VisualTab:AddSection({ "Visual Options" })

VisualTab:AddToggle({
	Name = "Full Bright",
	Default = false,
	Callback = function(state)
		if state then
			game:GetService("Lighting").Brightness = 5
			game:GetService("Lighting").ClockTime = 12
		end
	end
})MainTab:AddButton({
	Name = "No Clip",
	Callback = function()
		getgenv().Noclip = true
	end
})

RunService.Stepped:Connect(function()
	if getgenv().Noclip and player.Character then
		for _, v in pairs(player.Character:GetDescendants()) do
			if v:IsA("BasePart") then
				v.CanCollide = false
			end
		end
	end
end)local FunTab = Window:MakeTab({ "Fun", "star" })
FunTab:AddSection({ "Fun Scripts" })

FunTab:AddButton({
	Name = "Dance",
	Callback = function()
		local hum = player.Character:FindFirstChildOfClass("Humanoid")
		if hum then
			hum:LoadAnimation(Instance.new("Animation")).AnimationId = "rbxassetid://507771019"
		end
	end
})VisualTab:AddButton({
	Name = "Remove Fog",
	Callback = function()
		game:GetService("Lighting").FogEnd = 100000
	end
})

VisualTab:AddButton({
	Name = "Reset Lighting",
	Callback = function()
		game:GetService("Lighting"):ClearAllChildren()
	end
})local TeleportTab = Window:MakeTab({ "Teleport", "map-pin" })
TeleportTab:AddSection({ "Quick Teleports" })

TeleportTab:AddButton({
	Name = "Spawn",
	Callback = function()
		player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 5, 0)
	end
})

TeleportTab:AddButton({
	Name = "Random Place",
	Callback = function()
		player.Character.HumanoidRootPart.CFrame =
			CFrame.new(math.random(-500,500), 10, math.random(-500,500))
	end
})FunTab:AddButton({
	Name = "Spin Character",
	Callback = function()
		local hrp = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
		if hrp then
			local spin = Instance.new("BodyAngularVelocity")
			spin.AngularVelocity = Vector3.new(0, 50, 0)
			spin.MaxTorque = Vector3.new(0, math.huge, 0)
			spin.Parent = hrp
			task.delay(3, function()
				spin:Destroy()
			end)
		end
	end
})local MiscTab = Window:MakeTab({ "Misc", "settings" })
MiscTab:AddSection({ "Miscellaneous" })

MiscTab:AddButton({
	Name = "Reset Character",
	Callback = function()
		player.Character:BreakJoints()
	end
})

MiscTab:AddToggle({
	Name = "Anti AFK",
	Default = true,
	Callback = function(state)
		getgenv().AntiAFK = state
	end
})local VirtualUser = game:GetService("VirtualUser")

player.Idled:Connect(function()
	if getgenv().AntiAFK then
		VirtualUser:Button2Down(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
		task.wait(1)
		VirtualUser:Button2Up(Vector2.new(0,0), workspace.CurrentCamera.CFrame)
	end
end)Window:SelectTab(InfoTab)

